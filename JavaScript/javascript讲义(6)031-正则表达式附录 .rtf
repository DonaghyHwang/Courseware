function checkDate( value ) \{\par
if(value=='') return true;\par
if(value.length!=8 || !isNumber(value)) return false; \par
var year = value.substring(0,4);\par
if(year>"2100" || year< "1900")\par
return false;\par
\par
var month = value.substring(4,6);\par
if(month>"12" || month< "01") return false;\par
\par
var day = value.substring(6,8);\par
if(day>getMaxDay(year,month) || day< "01") return false;\par
\par
return true; \par
\}\par
