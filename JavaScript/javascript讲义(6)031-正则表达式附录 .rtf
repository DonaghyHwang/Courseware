function cTrim(sInputString,iType)\par
\{\par
var sTmpStr = ' ';\par
var i = -1;\par
\par
if(iType == 0 || iType == 1)\par
\{\par
while(sTmpStr == ' ')\par
\{\par
++i;\par
sTmpStr = sInputString.substr(i,1);\par
\}\par
sInputString = sInputString.substring(i);\par
\}\par
\par
if(iType == 0 || iType == 2)\par
\{\par
sTmpStr = ' ';\par
i = sInputString.length;\par
while(sTmpStr == ' ')\par
\{\par
--i;\par
sTmpStr = sInputString.substr(i,1);\par
\}\par
sInputString = sInputString.substring(0,i+1);\par
\}\par
return sInputString;\par
\} \par
